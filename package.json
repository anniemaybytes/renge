    "level": "8.0.1",
    "loglevel": "1.9.2",
    "loglevel-plugin-prefix": "0.8.4",
    "source-map-support": "0.5.21",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@types/chai": "5.2.2",
    "@types/mocha": "10.0.10",
    "@types/mock-fs": "4.13.4",
    "@types/node": "22.18.10",
    "@types/sinon": "17.0.4",
    "@typescript-eslint/eslint-plugin": "8.46.1",
    "@typescript-eslint/parser": "8.46.1",
    "c8": "10.1.3",
    "chai": "6.2.0",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "globals": "16.4.0",
    "mocha": "11.7.4",
    "mock-fs": "5.5.0",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "sinon": "21.0.0",
    "tsx": "4.20.6",
    "typescript": "5.9.3"
  }
